pipeline {
    agent any
    stages {
	stage('before_install') {
	  timestamps { 
	  steps {
		sh 'npm config set spin false'
                sh 'npm config set progress false'
	   }
	   }
	}
	stage('install') {
	  timpestamps {	   
	   steps {
		sh 'npm install'
	   }
 	  }
	}
        stage('test') {
          timestamps {
	    steps {
                sh 'npm run-script test'
            }
          }
        }
    }
}
